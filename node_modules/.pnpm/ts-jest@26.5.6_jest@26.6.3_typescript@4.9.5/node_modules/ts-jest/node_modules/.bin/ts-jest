#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xilosada/mero-js/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.9.5/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xilosada/mero-js/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.9.5/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
