#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules:/Users/xilosada/mero-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cnakazawa/watch/cli.js" "$@"
else
  exec node  "$basedir/../@cnakazawa/watch/cli.js" "$@"
fi
